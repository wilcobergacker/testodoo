<?xml version="1.0" encoding="utf-8"?>
<odoo>

        <record model="ir.module.category" id="module_category_subscription_planning">
            <field name="name">Subscription Planning</field>
            <field name="sequence">10</field>
        </record>

        <record id="subscription_planning_user" model="res.groups">
            <field name="name">User</field>
            <field name="category_id" ref="module_category_subscription_planning"/>
        </record>

        <record id="subscription_planning_manager" model="res.groups">
            <field name="name">Manager</field>
            <field name="implied_ids" eval="[(4, ref('subscription_planning_user'))]"/>
            <field name="category_id" ref="module_category_subscription_planning"/>
        </record>

</odoo>
