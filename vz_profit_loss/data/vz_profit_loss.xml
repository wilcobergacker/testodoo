<odoo noupdate="0">

    <!-- Vitamine & Zo - Winst & Verlies -->
    <record id="account_financial_report_vz_profitloss" model="account.financial.html.report">
        <field name="name">Vitamine &amp; Zo - Winst &amp; Verlies</field>
        <field name="debit_credit" eval="False"/>
        <field name="analytic" eval="True"/>
        <field name="unfold_all_filter" eval="True"/>
        <field name="show_journal_filter" eval="True"/>
        <field name="report_type">date_range_analytic</field>
        <field name='parent_id' ref="account.account_reports_legal_statements_menu"/>
    </record>

    <!-- Netto-winst -->
    <record id="account_financial_report_nw" model="account.financial.html.report.line">
        <field name="name">Netto-Winst</field>
        <field name="code">NW</field>
        <field name="financial_report_id" ref="account_financial_report_vz_profitloss"/>
        <field name="formulas">balance = VZ500.balance + VZ520.balance + VZ530.balance + VZ540.balance + VZ550.balance</field>
        <field name="sequence" eval="0"/>
        <field name="level" eval="0" />
    </record>

