        <field name="name">Bruto-marge</field>
        <field name="code">VZ500</field>
        <field name="parent_id" ref="account_financial_report_nw"/>
        <field name="formulas">balance = VZ50005.balance + VZ50015.balance + VZ50035.balance</field>
        <field name="sequence" eval="1"/>
        <field name="level" eval="1" />
    </record>
    <record id="account_financial_report_50005" model="account.financial.html.report.line">
        <field name="name">Netto-omzet</field>
        <field name="code">VZ50005</field>
        <field name="parent_id" ref='account_financial_report_500'/>
        <field name="domain" eval="[[u'account_id.group_id', u'ilike', u'50005']]" />
        <field name="groupby">account_id</field>
        <field name="formulas">balance = -sum.balance</field>
        <field name="sequence" eval="1"/>
        <field name="level" eval="2" />
        <field name="show_domain">foldable</field>
    </record>
